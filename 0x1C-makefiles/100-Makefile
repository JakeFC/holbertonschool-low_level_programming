CC = gcc
SRC = main.c holberton.c
OBJ = $(SRC:.c=.o)
NAME = holberton
CFLAGS = -Wall -Werror -Wextra -pedantic

.PHONY: all $(NAME) clean oclean fclean re
$(NAME): $(OBJ)
	$(CC) -o $(NAME) $(OBJ)
all: m.h $(NAME)
clean:
	$(RM) -f *~ $(NAME)
oclean:
	$(RM) -f $(OBJ)
fclean: clean oclean
re: oclean all
